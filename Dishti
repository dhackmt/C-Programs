
#include<stdio.h>
#include<conio.h>
void main()
{
	int arr[10],i,n,sum=0;
	clrscr();
	printf("enter size");
	scanf("%d",&n);
	printf("enter n elements");
	for(i=0;i<n;i++)
	{
	   scanf("%d",&arr[i]);
	}
	printf("\tarray is :");
	for(i=0;i<n;i++)
	{
	   printf("%d",arr[i]);
	   sum=sum+arr[i];
	}
	printf("sum is %d",sum);

	getch();
}
